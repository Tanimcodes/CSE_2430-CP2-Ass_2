
#include<bits/stdc++.h>
using namespace std;

int main()
{

  int t;
  cin>>t;

  while(t--){
   string s;
   cin>> s;


   int freq [26] = {0};

   for(char c : s){
    freq[c- 'a']++;
   }

   int atLeast2 = 0, exactly1 = 0;


   for(int i =0;i<26;i++){
    if(freq[i] >=2)
         atLeast2++;

    else if(freq[i] >=1)
        exactly1++;
   }


   int pairs = atLeast2 + (exactly1/ 2);
   cout<< pairs<<endl;


  }
}
